{
  "forge_marker": 1,
  "multipart": [
    {   "apply": { "model": "pane_post", "textures": { "edge": "ariente:blocks/decorative/marble_fence_top", "pane": "ariente:blocks/decorative/marble_fence" } }},
    {   "when": { "north": true },
      "apply": { "model": "pane_side", "textures": { "edge": "ariente:blocks/decorative/marble_fence_top", "pane": "ariente:blocks/decorative/marble_fence" } }
    },
    {   "when": { "east": true },
      "apply": { "model": "pane_side", "y": 90, "textures": { "edge": "ariente:blocks/decorative/marble_fence_top", "pane": "ariente:blocks/decorative/marble_fence" } }
    },
    {   "when": { "south": true },
      "apply": { "model": "pane_side_alt", "textures": { "edge": "ariente:blocks/decorative/marble_fence_top", "pane": "ariente:blocks/decorative/marble_fence" } }
    },
    {   "when": { "west": true },
      "apply": { "model": "pane_side_alt", "y": 90, "textures": { "edge": "ariente:blocks/decorative/marble_fence_top", "pane": "ariente:blocks/decorative/marble_fence" } }
    },
    {   "when": { "north": false },
      "apply": { "model": "pane_noside", "textures": { "pane": "ariente:blocks/decorative/marble_fence" } }
    },
    {   "when": { "east": false },
      "apply": { "model": "pane_noside_alt", "textures": { "pane": "ariente:blocks/decorative/marble_fence" } }
    },
    {   "when": { "south": false },
      "apply": { "model": "pane_noside_alt", "y": 90, "textures": { "pane": "ariente:blocks/decorative/marble_fence" } }
    },
    {   "when": { "west": false },
      "apply": { "model": "pane_noside", "y": 270, "textures": { "pane": "ariente:blocks/decorative/marble_fence" } }
    }
  ]
}
